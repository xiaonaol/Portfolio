        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>editBean</managed-bean-name>
        <managed-bean-class>com.example.jsf.Bean.controller.EditBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
